{
	"name": "spotter",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"db:push": "pnpm 'dlx' @better-auth/cli generate --yes --output ./src/lib/server/db/auth.ts && drizzle-kit push",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@lucide/svelte": "^0.561.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"@threlte/core": "^8.3.1",
		"@threlte/extras": "^9.7.1",
		"@types/bun": "^1.3.4",
		"@types/three": "^0.182.0",
		"better-auth": "^1.4.7",
		"bits-ui": "^2.14.4",
		"mode-watcher": "^1.1.0",
		"phosphor-svelte": "^3.0.1",
		"pnpm": "^10.25.0",
		"runed": "^0.37.0",
		"three": "^0.182.0",
		"valibot": "^1.2.0"
	},
	"packageManager": "pnpm@10.25.0"
}
